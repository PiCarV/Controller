{
  "author": {
    "name": "Andre Cox",
    "email": "andrecox@ubc.ca"
  },
  "name": "picar-controller",
  "description": "An application to control a PiCar",
  "homepage": "https://PiCarV.github.io/Controller",
  "version": "1.0.0",
  "main": "electron/main.ts",
  "scripts": {
    "start": "concurrently --kill-others \"snowpack dev\" \"electron .\"",
    "build": "snowpack build && electron-builder build --publish never",
    "test": "web-test-runner \"src/**/*.test.tsx\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.2",
    "builtin-modules": "^3.3.0",
    "electron-is": "^3.0.0",
    "electron-store": "^8.0.1",
    "gamepads": "^1.2.2",
    "gh-pages": "^4.0.0",
    "mobx": "^6.3.13",
    "mobx-react-lite": "^3.2.3",
    "react": "^17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-gamepads": "^1.0.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.1",
    "socket.io-client": "^4.4.1",
    "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store",
    "universal-cookie": "^4.0.4",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.1.0",
    "@snowpack/plugin-postcss": "^1.4.3",
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "@snowpack/plugin-sass": "^1.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/plugin-webpack": "^3.0.0",
    "@snowpack/web-test-runner-plugin": "^0.2.2",
    "@tauri-apps/cli": "^1.0.0-rc.13",
    "@testing-library/react": "^11.2.6",
    "@types/chai": "^4.2.17",
    "@types/mocha": "^8.2.2",
    "@types/node": "^17.0.41",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/snowpack-env": "^2.3.3",
    "@types/validator": "^13.7.1",
    "@web/test-runner": "^0.13.3",
    "chai": "^4.3.4",
    "concurrently": "^7.2.1",
    "electron": "^19.0.3",
    "electron-builder": "^23.0.3",
    "electron-snowpack": "^0.13.0",
    "prettier": "^2.2.1",
    "snowpack": "^3.3.7",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "productName": "PiCar Controller",
    "appId": "com.PiCarController.AndreCox",
    "files": [
      "package.json",
      "build/**/*",
      "electron/main.ts"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "output": "dist"
    }
  }
}
